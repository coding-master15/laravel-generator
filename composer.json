{
    "name": "coding-master15/laravel-generator",
    "description": "Smarter Vision Generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Smarter Vision",
            "email": "smartersvision@gmail.com"
        },
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.13.0",
        "prettus/l5-repository": "^2.9.0",
        "laracasts/flash": "^3.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.3",
        "mockery/mockery": "^1.5.1"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    }    
}
